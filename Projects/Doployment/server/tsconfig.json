{ 
"compilerOptions": {
  "target": "ES2020",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
  "sourceMap": true,
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "outDir": "./dist",
  "baseUrl": ".",
  "resolveJsonModule": true
},
"include": ["src/**/*.ts"],
"exclude": ["node_modules"]
}